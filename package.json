{
  "name": "derekmisler.com",
  "version": "3.1.0",
  "private": true,
  "repository": "https://github.com/derekmisler/derekmisler.com",
  "homepage": "https://portfolio-derek-misler.firebaseapp.com",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "npx next",
    "build": "npx next build && next export",
    "start": "npx next start",
    "stylelint": "npx stylelint './**/*.tsx'",
    "format": "npx prettier --write '**/*.{ts,tsx}'",
    "deploy": "npm run build && firebase deploy",
    "deploy:ci": "firebase deploy --only hosting --token ${{ secrets.FIREBASE_TOKEN }}",
    "clean:all": "rm -rf out && rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm i --no-audit"
  },
  "dependencies": {
    "firebase": "^7.2.1",
    "next": "latest",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-ga": "^2.7.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@types/node": "12.0.12",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.16",
    "babel-plugin-styled-components": "^1.10.0",
    "prettier": "1.18.2",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "3.5.2"
  }
}
