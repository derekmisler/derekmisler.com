    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.9",
    "babel-plugin-styled-components": "^1.8.0",
    "prettier": "1.18.2",
    "typescript": "^3.6.4"
  },
  "license": "ISC"
}
