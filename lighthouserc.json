{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "staticDistDir": "./.next",
      "startServerCommand": "npm run serve:lhci",
      "url": [
        "http://localhost:3000"
      ]
    },
    "assert": {
      "preset": "lighthouse:all"
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
