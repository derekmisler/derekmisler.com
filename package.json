{
  "name": "derekmisler.com",
  "version": "3.0.1",
  "private": false,
  "repository": "https://github.com/derekmisler/derekmisler.com",
  "homepage": "https://portfolio-derek-misler.firebaseapp.com",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node scripts/start.js",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "lint": "eslint --fix-dry-run ./src/*",
    "build": "node scripts/build.js && cp build/index.html build/404.html",
    "deploy": "npm run build && firebase deploy",
    "styles": "stylus -u autoprefixer-stylus ./src/assets/css/style.styl -o ./src/assets/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/assets/css/style.styl -o ./src/assets/css/style.css",
    "clean:everything": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm install"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "autoprefixer": "^6.4.1",
    "autoprefixer-stylus": "^0.10.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-airbnb": "^2.4.0",
    "chalk": "1.1.3",
    "concurrently": "3.0.0",
    "css-loader": "0.24.0",
    "detect-port": "1.0.0",
    "dotenv": "2.0.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.1",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "0.13.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "url-loader": "0.5.7",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  }
}
